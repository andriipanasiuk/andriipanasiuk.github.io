---
layout: layout
---

<div class="content">
  <h1>
    {{ page.title }}
  </h1>

  {{ content }}
  <ul class="listing">
  {% for post in site.categories.english %}
    {% unless post.draft %}
    <li>
      <span>{{ post.date | date: "%B %e, %Y" }}</span>
      <a href="{{ post.url }}">{{ post.title }}</a>
    </li>
    {% endunless %}
  {% endfor %}
  </ul>
</div>